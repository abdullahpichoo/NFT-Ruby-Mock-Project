<%= form_with(model: [@wallet,nft], html: { multipart: true }) do |form| %>
  <% if nft.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(nft.errors.count, "error") %> prohibited this nft from being saved:</h2>
      <ul>
        <% nft.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :price, style: "display: block" %>
    <%= form.text_field :price %>
  </div>
  <div>
    <%= form.label :image %>
    <%= form.file_field :images %>
  </div>
  <div>
    <% form.label :owner, style: "display: block" %>
    <%= form.hidden_field :owner ,value:current_user.username %>
  </div>
  <div>
    <% form.label :created_by, style: "display: block" %>
    <%= form.hidden_field :created_by,value: current_user.username %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
